{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths":{
        "components/*": ["src/components/*"],
        "src/*": ["src/*"],
        "screens/*": ["src/screens/*"],
        "config/*": ["src/config/*"],
        "types/*": ["src/types/*"],
    }
  }
}
